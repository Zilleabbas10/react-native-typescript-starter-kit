{
  "name": "StarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll --verbose",
    "test:coverage": "NODE_ENV=test jest --verbose --coverage",
    "lint": "eslint .",
    "compile": "rm -rf dist && tsc",
    "android:clean": "cd android && ./gradlew clean",
    "android:assembleRelease": "cd android && ./gradlew assembleRelease",
    "android:assembleDebug": "cd android && ./gradlew assembleDebug",
    "android:release": "npm run compile && npm run android:clean && npm run android:assembleRelease",
    "android:debug": "npm run compile && npm run android:clean && npm run android:assembleDebug",
    "android:dev": "concurrently \"tsc -w\" \"npm run android:clean && react-native run-android\"",
    "ios:dev": "concurrently \"tsc -w\" \"react-native run-ios\"",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.1.7",
    "@react-navigation/stack": "^5.2.14",
    "@types/redux-logger": "^3.0.7",
    "axios": "^0.19.2",
    "axios-hooks": "^1.10.0",
    "concurrently": "^5.2.0",
    "formik": "^2.1.4",
    "ramda": "^0.27.0",
    "react": "16.11.0",
    "react-if": "^3.4.3",
    "react-native": "0.62.2",
    "react-native-cookies": "^3.3.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rn-progress-loader": "^1.0.8",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.4",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
